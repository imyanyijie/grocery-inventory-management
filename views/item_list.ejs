<%- include('header') %>
<div class="container-fluid text-center">    
  <div class="row content">
    <div class="col-sm-8 text-left"> 
      <h1><%- title %></h1>
      <% if(items.length){ %>
      <ul>
        <%for(let item of items){%>
          <li><a href=<%- item.url %>><%- item.name %></a></li>
          <% } %>
      </ul>
      <% } %>
      <hr>
      <p><a href="/item/create">Create Item</a></p>
    </div>
  </div>
</div>